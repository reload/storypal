{
    "name": "reload/storypal",
    "description": "Storybook + Drupal - Project boilerplate.",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Reload A/S",
            "role": "Development"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://git.drupalcode.org/issue/services_env_parameter-3276633.git"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.1",
        "drupal/allowed_formats": "^1.3",
        "drupal/components": "^2.4",
        "drupal/cookiebot": "^1.0@alpha",
        "drupal/core-composer-scaffold": "^9.2",
        "drupal/core-project-message": "^9.2",
        "drupal/core-recommended": "^9.2",
        "drupal/editor_advanced_link": "^1.9",
        "drupal/field_group": "^3.2",
        "drupal/focal_point": "^1.5",
        "drupal/gin": "^3.0",
        "drupal/gin_login": "^1.0",
        "drupal/gin_toolbar": "^1.0@beta",
        "drupal/gzip_html": "^1.2",
        "drupal/jquery_ui_accordion": "^1.1",
        "drupal/jquery_ui_multiselect_widget": "1.x-dev@dev",
        "drupal/linkit": "^5",
        "drupal/media_library_edit": "^2.1",
        "drupal/metatag": "^1.19",
        "drupal/paragraphs": "^1.12",
        "drupal/paragraphs_ee": "^1.8",
        "drupal/pathauto": "^1.8",
        "drupal/quick_node_clone": "^1.14",
        "drupal/redirect": "^1.6",
        "drupal/redis": "^1.5",
        "drupal/simple_sitemap": "^4.1",
        "drupal/stage_file_proxy": "^1.1",
        "drupal/twig_tweak": "^3.1",
        "drupal/workflow_buttons": "^1.0@beta",
        "drush/drush": "^10",
        "firebase/php-jwt": "^6.1",
        "predis/predis": "^1.1",
        "sainsburys/guzzle-oauth2-plugin": "^3.0"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
        "drupal/coder": "^8.3",
        "drupal/core-dev": "^9.2",
        "jangregor/phpstan-prophecy": "^1.0",
        "mglaman/phpstan-drupal": "^1",
        "phpspec/prophecy-phpunit": "^2",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1",
        "phpstan/phpstan-deprecation-rules": "^1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "symfony": {
            "require": "3.4.*|^4.3"
        },
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "patches": {
            "drupal/gin": {
                "CUSTOM: Show actions to fix workflow_buttons": "patches/gin_show_actions.patch"
            },
            "drupal/paragraphs": {
                "CUSTOM: Add remove button to actions": "patches/paragraphs_remove-button.patch"
            },
            "drupal/paragraphs_ee": {
                "3106254: Sorting paragraph list alphabetically in modal": "https://www.drupal.org/files/issues/2022-02-25/3106254_6.patch",
                "CUSTOM: change inline-label for show all": "patches/paragraphs_ee_show_all.patch"
            },
            "drupal/core": {
                "3073554: Add a token for publication status": "https://www.drupal.org/files/issues/2019-08-09/3073554_10.patch",
                "3090668: Log errors for missing route_name": "https://www.drupal.org/files/issues/2021-04-05/3090668-23_1.patch",
                "2719797: New option for Views page displays to use the admin theme": "https://www.drupal.org/files/issues/2021-05-06/2053399-126.patch",
                "3247551: user.admin_permissions has a 'wrong' permission": "https://www.drupal.org/files/issues/2021-11-03/d9_user.admin_permissions.patch",
                "3269308: 'Add media' button is too general": "https://www.drupal.org/files/issues/2022-03-13/3269308-media_library_better_add_label_9-3.patch"
            },
            "drupal/cookiebot": {
                "3271014: Async causes major issues in Safari": "https://www.drupal.org/files/issues/2022-03-22/cookiebot-no-async.patch"
            },
            "drupal/jquery_ui_multiselect_widget": {
                "2941549: Rewrite the Drupal 8/9 branch": "https://www.drupal.org/files/issues/2021-10-14/2941549_rewrite-d8d9-version.patch"
            },
            "drupal/field_group": {
                "3205504: Add support for #description_display for container types that support it": "https://www.drupal.org/files/issues/2021-08-24/3205504-6.field_group_description_display.patch"
            },
            "drupal/linkit": {
                "2712951: Linkit for Link field": "https://www.drupal.org/files/issues/2022-02-09/2712951-274.patch"
            }
        }
    }
}
